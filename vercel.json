{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/asset(.*)",
      "dest": "/"
    },
    {
      "src": "/asset/css/(.*)",
      "dest": "bootstrap.min.css"
    }
  ],
  "buildCommand": "npm install --prefix backend && npm install --prefix frontend && npm run build --prefix frontend"
}
